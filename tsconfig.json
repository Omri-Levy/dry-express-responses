{
  "compilerOptions": {
	"lib": [
	  "ESNext"
	],
	"rootDir": "src",
	"module": "commonjs",
	"target": "es5",
	"strict": true,
	"forceConsistentCasingInFileNames": true,
	"moduleResolution": "node",
	"skipLibCheck": true,
	"importHelpers": true,
	"isolatedModules": true,
	"outDir": "dist",
	"declaration": true,
	"declarationMap": true,
	"esModuleInterop": true,
	"types": [
	  "bun-types"
	]
  },
  "include": [
	"src/**/*.ts",
  ],
  "exclude": [
	"node_modules"
  ]
}
